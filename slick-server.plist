<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>io.slick.slick-server</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
        <string>$SLICK_INSTALL_HOME/slick/bin/slick-server</string>
    </array>
    <key>WorkingDirectory</key>
    <string>$SLICK_ROOT</string>
    <key>StandardErrorPath</key>
    <string>$SLICK_ROOT/server.log</string>
    <key>StandardOutPath</key>
    <string>$SLICK_ROOT/server.log</string>
    <key>UserName</key>
    <string>$USER</string>
  </dict>
</plist>
