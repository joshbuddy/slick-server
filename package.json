{
  "name": "slick-server",
  "version": "0.1.0",
  "description": "Server for encrypted streamable file-system backed by your own cloud storage",
  "preferGlobal": "true",
  "bin": {
    "slick": "bin/slick",
    "slick-add": "bin/slick-add",
    "slick-cat": "bin/slick-cat",
    "slick-config": "bin/slick-config",
    "slick-cp": "bin/slick-cp",
    "slick-fetch": "bin/slick-fetch",
    "slick-help": "bin/slick-help",
    "slick-info": "bin/slick-info",
    "slick-ls": "bin/slick-ls",
    "slick-mkdir": "bin/slick-mkdir",
    "slick-mv": "bin/slick-mv",
    "slick-ops": "bin/slick-ops",
    "slick-rm": "bin/slick-rm",
    "slick-server": "bin/slick-server",
    "slick-volumes": "bin/slick-volumes"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshbuddy/slick-server.git"
  },
  "scripts": {
    "prepublish": "make docs"
  },
  "directories": {
    "man": "./man"
  },
  "author": "Josh Hull",
  "license": "BSD-2-Clause",
  "dependencies": {
    "slick-io": "*",
    "aws-sdk": "2.2.32",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "diskusage": "0.1.4",
    "expand-home-dir": "0.0.3",
    "express": "4.13.4",
    "fs-extra": "0.26.4",
    "glob": "6.0.4",
    "http-server": "0.8.5",
    "humanize": "0.0.9",
    "leveldown": "1.4.3",
    "levelup": "1.3.1",
    "lockfile": "1.0.1",
    "lodash": "4.0.0",
    "loglevel": "1.4.0",
    "lru-cache": "4.0.0",
    "mime": "1.3.4",
    "node-dir": "0.1.11",
    "node-uuid": "1.4.7",
    "progress": "1.1.8",
    "prompt": "0.2.14",
    "scryptsy": "1.2.1",
    "sodium": "1.0.22",
    "sprintf-js": "1.0.3",
    "strftime": "0.9.2",
    "body-parser": "1.14.2",
    "blake2": "1.2.2",
    "backoff": "2.4.1",
    "eventsource": "0.1.6",
    "request": "2.67.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "marked-man": "0.1.5",
    "mocha": "2.3.4",
    "supertest": "1.1.0"
  },
  "optionalDependencies": {
    "fuse-bindings": "2.8.1"
  },
  "engines": {
    "node": ">=4.2.0 <5.0.0"
  }
}
